# Base output directory for generated primers (preferred)
OUTPUT_BASE_DIR=C:/path/to/commercial-primer-output

# Legacy fallback (used only if OUTPUT_BASE_DIR is not set)
OUTPUT_DIR=C:/path/to/legacy-output

# Optional lead input path (defaults to ./lead_input.json if not set)
LEAD_INPUT_PATH=./lead_input.json

# Word template (optional)
PRIMER_WORD_TEMPLATE_PATH=templates/Commercial_Primer_Template.docx

# OpenAI
OPENAI_API_KEY=
OPENAI_MODEL=gpt-5.2
OPENAI_DEEP_RESEARCH_MODEL=o4-mini-deep-research
OPENAI_MAX_RETRIES=6
OPENAI_RETRY_BASE_SECONDS=0.5

# Prompt library
PROMPT_LIBRARY_PATH=prompts/_private/PROMPTS LIBRARY COMMERCIAL v03.xlsx

# Optional integrations
HUBSPOT_API_KEY=
